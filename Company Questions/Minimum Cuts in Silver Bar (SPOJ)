#include<iostream>
using namespace std;
int main()
{
        int n,count;
        cin>>n;
        while(n!=0)
        {
                count=0;
                while(n>1)
                {
                        n=n/2;
                        count+=1;
                }
                cout<<count<<endl;
                cin>>n;
        }
        return 0;
}
