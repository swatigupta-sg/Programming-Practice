class MyHashSet {
public:
    bool arr[1000001] = {0};
    /** Initialize your data structure here. */
    MyHashSet() {
        
    }
    
    void add(int key) {
        arr[key] = true; 
    }
    
    void remove(int key) {
        arr[key] = false;
    }
    
    /** Returns true if this set contains the specified element */
    bool contains(int key) {
        return arr[key];
    }
};
